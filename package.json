{
  "name": "eth-alarm-clock-dapp",
  "version": "1.0.0-rc.1",
  "main": "main.js",
  "description": "",
  "scripts": {
    "test": "jest",
    "test-watch": "jest -u --watchAll",
    "build": "webpack --config webpack/prod.config.js",
    "build:electron:dev": "webpack --config webpack/electron.dev.config.js",
    "build:electron:prod": "webpack --config webpack/electron.prod.config.js",
    "dev-docker": "node_modules/.bin/webpack-dev-server --config webpack/docker.dev.config.js --host 0.0.0.0 --disable-host-check",
    "dev": "node_modules/.bin/webpack-dev-server --config webpack/dev.config.js --host 0.0.0.0 --disable-host-check",
    "dev:electron": "concurrently \"npm run dev\" \"wait-on http://localhost:8080 && electron tools/electron.js\"",
    "prod": "node_modules/.bin/webpack-dev-server --config webpack/prod.config.js --host 0.0.0.0 --disable-host-check --hot",
    "debug-prod": "webpack --config webpack/debug.prod.config.js",
    "lint": "eslint .",
    "lint-fix": "eslint --fix .",
    "clean": "./tools/clean.sh",
    "validate": "npm ls",
    "docker-build": "docker-compose build && docker-compose up -d",
    "docker-build:nocache": "docker-compose build --no-cache && docker-compose up -d",
    "docker-remove": "docker-compose down -v",
    "docker-rebuild": "npm run docker-remove && npm run docker-build",
    "docker-rebuild:nocache": "npm run docker-remove && npm run docker-build:nocache",
    "package:win": "npm run build && electron-packager . --overwrite --icon=electron-app/icons/icon.ico --platform=win32",
    "package:linux": "npm run build && electron-packager . --overwrite --platform=linux",
    "package:mac": "npm run build && electron-packager . --overwrite --icon=electron-app/icons/icon.icns --platform=darwin",
    "precommit": "lint-staged",
    "electron:distribute": "npm run build:electron:prod && node tools/buildElectron.js",
    "formatAll": "find ./app/ ./__tests__/ -name '*.js*' | xargs prettier --write --config ./.prettierrc --config-precedence file-override"
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "prettier --write --config ./.prettierrc --config-precedence file-override",
      "git add"
    ]
  },
